<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
<!-- Main -->
    <Script file="EN_UnitFrames.lua"/>

<!-- Localization -->
    <Script file="EN_UnitFramesLocal.lua"/>

<!-- Independent scripts -->
    <Script file="EN_Buff.lua"/>

<!-- General frame script -->
    <Frame name="EUF_General" parent="UIParent" hidden="true">
        <Scripts>
            <OnLoad>
                EUF_OnLoad(self);
            </OnLoad>
            <OnEvent>
                EUF_OnEvent(self, event, ...);
            </OnEvent>
        </Scripts>
    </Frame>
</Ui>