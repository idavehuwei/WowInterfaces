		<Scripts>
			<OnLoad>
				LibQuestInfoTileButton_OnLoad(self);
			</OnLoad>
			<OnClick>
				--LibQuestInfoTileButton_OnClick(self)
			</OnClick>
			<OnEnter>
				if self.qID then
					LibQuestInfoEditorTooltip:ClearLines();
					LibQuestInfoEditorTooltip:SetOwner(self, ANCHOR_RIGHT);
					LibQuestInfoEditorTooltip:SetHyperlink("quest:" .. self.qID)
				end
			</OnEnter>
			<OnLeave>
				if LibQuestInfoEditorTooltip:IsVisible() then
					LibQuestInfoEditorTooltip:Hide()
				end
			</OnLeave>
		</Scripts>
	</Button>
	
