	<Button name="GameTooltipTemplatetestbutton" parent="UIParent"  text="Open QuestEditor" inherits="UIPanelButtonTemplate">
		<ButtonText name="$parentText"/>
		<NormalFont style="GameFontNormalSmall"/>
		<HighlightFont style="GameFontHighlightSmall"/>
		<DisabledFont style="GameFontDisableSmall"/>
		<Size>
			<AbsDimension x="100" y="20"/>
		</Size>		
		<Anchors>
			<Anchor point="TOPRIGHT">
				<Offset>
					<AbsDimension x="0" y="-250"/>
				</Offset>
			</Anchor>
		</Anchors>
		<Scripts>
			<OnClick>
					if LibQuestInfoFrame:IsVisible() then
						LibQuestInfoFrame:Hide()
					else
						LibQuestInfoFrame:Show()
					end
			</OnClick>
		</Scripts>
	</Button>

