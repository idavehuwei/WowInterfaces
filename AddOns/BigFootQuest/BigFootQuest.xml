<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/..\FrameXML\UI.xsd">	
	<Script file="BFQData.lua"/>
	<Script file="BFQData.cn.lua"/>
	<Script file="BFQData.tw.lua"/>
	<Script file="BigFootQuest.lua"/>

	<Button name="BigFootQuestObjectiveButtonTemplate" hidden="true" virtual="true">
		<Layers>
			<Layer level="BACKGROUND">
				<Texture file="Interface\WorldMap\WorldMap-MagnifyingGlass">
					<Size>
						<AbsDimension x="19" y="19"/>
					</Size>
					<Anchors>
						<Anchor point="RIGHT">
							<Offset>
								<AbsDimension x="-20" y="-2"/>
							</Offset>
						</Anchor>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
		<NormalTexture >
			<Size>
				<AbsDimension x="275" y="14"/>
			</Size>
			<Anchors>
				<Anchor point="CENTER" relativePoint="CENTER"/>
			</Anchors>
		</NormalTexture>
		<PushedTexture alphaMode="ADD" file="Interface\QuestFrame\UI-QuestTitleHighlight">
			<Size>
				<AbsDimension x="275" y="14"/>
			</Size>
			<Anchors>
				<Anchor point="CENTER" relativePoint="CENTER"/>
			</Anchors>
		</PushedTexture>
		<HighlightTexture alphaMode="ADD" file="Interface\QuestFrame\UI-QuestTitleHighlight">
			<Size>
				<AbsDimension x="275" y="14"/>
			</Size>
			<Anchors>
				<Anchor point="CENTER" relativePoint="CENTER"/>
			</Anchors>
		</HighlightTexture>
		<Scripts>
			<OnLoad>
				self:RegisterForClicks("LeftButtonUp", "RightButtonUp");
			</OnLoad>
			<OnClick>
				BigFootQuestObjectiveButton_OnClick(self, button);
			</OnClick>
		</Scripts>
	</Button>
	<Button name="BigFootQuestObjectiveButton1" inherits="BigFootQuestObjectiveButtonTemplate" parent="QuestLogDetailScrollChildFrame" id="1">
	</Button>
	<Button name="BigFootQuestObjectiveButton2" inherits="BigFootQuestObjectiveButtonTemplate" parent="QuestLogDetailScrollChildFrame" id="2">
	</Button>
	<Button name="BigFootQuestObjectiveButton3" inherits="BigFootQuestObjectiveButtonTemplate" parent="QuestLogDetailScrollChildFrame" id="3">
	</Button>
	<Button name="BigFootQuestObjectiveButton4" inherits="BigFootQuestObjectiveButtonTemplate" parent="QuestLogDetailScrollChildFrame" id="4">
	</Button>
	<Button name="BigFootQuestObjectiveButton5" inherits="BigFootQuestObjectiveButtonTemplate" parent="QuestLogDetailScrollChildFrame" id="5">
	</Button>
	<Button name="BigFootQuestObjectiveButton6" inherits="BigFootQuestObjectiveButtonTemplate" parent="QuestLogDetailScrollChildFrame" id="6">
	</Button>
	<Button name="BigFootQuestObjectiveButton7" inherits="BigFootQuestObjectiveButtonTemplate" parent="QuestLogDetailScrollChildFrame" id="7">
	</Button>
	<Button name="BigFootQuestObjectiveButton8" inherits="BigFootQuestObjectiveButtonTemplate" parent="QuestLogDetailScrollChildFrame" id="8">
	</Button>
</Ui>
