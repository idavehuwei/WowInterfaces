digraph LibGuildStorage {
  STALE
  STALE_WAITING_FOR_ROSTER_UPDATE
  CURRENT
  FLUSHING
  REMOTE_FLUSHING

  edge [fontcolor=blue]

  STALE_WAITING_FOR_ROSTER_UPDATE -> STALE [label="GUILD_ROSTER_UPDATE(nil) event"]
