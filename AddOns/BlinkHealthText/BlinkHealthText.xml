										<AbsDimension x="-30" y="-10"/>
									</Offset>
								</Anchor>
							</Anchors>
						</FontString>
					</Layer>
				</Layers>
				<Scripts>
					<OnLoad>
                        BlinkPowerTextTarget_OnLoad(self);
                    </OnLoad>
					<OnEvent>
                        BlinkPowerTextTarget_OnEvent(self, event);
                    </OnEvent>
				</Scripts>
			</Frame>
		</Frames>
		<Scripts>
			<OnLoad>
                BlinkHealthText_OnLoad(self);
            </OnLoad>
			<OnEvent>
                BlinkHealthText_OnEvent(self, event);
            </OnEvent>
		</Scripts>
	</Frame>
</Ui>
