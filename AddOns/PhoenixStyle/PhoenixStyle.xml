<Frame name="PhoenixStyleFailbot">
	<Scripts>
		<OnLoad>
			PhoenixStyle_OnLoad()
		</OnLoad>
		<OnEvent>
			if(thisaddonwork or (thisaddonwork==false and event=="ADDON_LOADED")) then 
				PhoenixStyle_OnEvent() 
			end
		</OnEvent>
		<OnUpdate>
			if(thisaddonwork) then 
				PSF_OnUpdate() 
			end
		</OnUpdate>
	</Scripts>
</Frame>

